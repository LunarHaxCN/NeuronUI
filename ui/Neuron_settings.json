{
  "type": "Menu",
  "title": {
    "name": "『Settings』",
    "size": 15,
    "elevation": 4,
    "background": "$TitleBG",
    "padding": [
      4,
      4,
      4,
      4
    ],
    "colors": [
      "$TitleColor",
      "$TitleColor"
    ],
    "margins": [0, 0, 0, 0]
  },
  "color": "$MenuColor",
  "alpha": 0.85,
  "can_close": true,
  "hide": true,
  "margins": [0, 0, 0, 0],
  "image_scaled_size": 40,
  "radius": 10,
  "show_dividers": false,
  "items": [
    {
      "type": "Switch",
      "name": "显示水印",
      "color": "$TextColor",
      "size": 12,
      "padding": [
        5,
        0,
        5,
        0
      ],
      "shortcut": {
        "type": "CheckedButton",
        "params": [65, 65],
        "no_circle": true,
        "off":"close/NeuronOff_WaterMark.png",
        "on":"open/NeuronOn_WaterMark.png",
        "icon":"open/NeuronOn_WaterMark.png"
      },
      "tag": "fun_watermark"
    },
    {
      "type": "Switch",
      "name": "加入游戏自动复制地址",
      "color": "$TextColor",
      "size": 12,
      "padding": [
        5,
        5,
        5,
        5
      ],
      "shortcut": {
        "type": "CheckedButton",
        "params": [65, 65],
        "no_circle": true,
        "off":"close/NeuronOff_CopyAddress.png",
        "on":"open/NeuronOn_CopyAddress.png",
        "icon":"open/NeuronOn_CopyAddress.png"
      },
      "tag": "fun_join_copy_address"
    },
    {
      "type": "Switch",
      "name": "悬浮窗口半屏显示",
      "color": "$TextColor",
      "size": 12,
      "padding": [
        5,
        5,
        5,
        5
      ],
      "checked": true,
      "register_setting": "setting_window_cut"
    },
    {
      "type": "Switch",
      "name": "隐藏菜单快捷按钮(长按)",
      "color": "$TextColor",
      "size": 12,
      "padding": [
        5,
        0,
        5,
        0
      ],
      "shortcut": {
        "type": "CheckedButton",
        "params": [65, 65],
        "no_circle": true,
        "off":"close/NeuronOff_HideMenu.png",
        "on":"open/NeuronOn_HideMenu.png",
        "icon":"open/NeuronOn_HideMenu.png"
      },
      "tag": "fun_hide_menu_shortcut"
    },
    {
      "type": "TextView",
      "name": "关于",
      "color": "$TextColor",
      "size": 12,
      "padding": [
        5,
        5,
        5,
        5
      ],
      "open": "Neuron_about"
    },
    {
      "type": "TextView",
      "name": "删除游戏数据",
      "color": "$ItemSet",
      "size": 12,
      "padding": [
        5,
        5,
        5,
        5
      ],
      "shortcut": {
        "type": "ClickButton",
        "params": [65, 65],
        "no_circle": true,
        "icon":"other/Neuron_DeleteAppData.png"
