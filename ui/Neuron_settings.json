      "color": "$ItemSet",
      "size": 12,
      "padding": [
        5,
        5,
        5,
        5
      ],
      "shortcut": {
        "type": "ClickButton",
        "params": [65, 65],
        "no_circle": true,
        "icon":"other/Neuron_ReactNativeOpen.png"
