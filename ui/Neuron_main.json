      "name": "JS Loader",
      "color": "$TextColor",
      "size": 12,
      "padding": [
        5,
        5,
        5,
        5
      ],
      "load_script": "JsLoader.js"
    },
    {
      "type": "TextView",
      "name": "Combat",
      "color": "$TextColor",
      "size": 12,
      "padding": [
        5,
        5,
        5,
        5
      ],
      "open": "Neuron_combat"
    },
    {
      "type": "TextView",
      "name": "Movement",
      "color": "$TextColor",
      "size": 12,
      "padding": [
        5,
