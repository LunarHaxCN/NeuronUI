      "type": "TextView",
      "name": "Settings",
      "color": "$TextColor",
      "size": 12,
      "padding": [
        5,
        5,
        5,
        5
      ],
      "open": "Neuron_settings"
    },
    {
      "type": "TextView",
      "name": "Hide All",
      "color": "$MenuHide",
      "size": 12,
      "padding": [
        5,
        5,
        5,
        5
      ],
      "tag": "fun_hide"
    },
    {
      "type": "TextView",
      "html": "UI作者：<a href=''>MoYuanCN</a>",
      "url": "https://github.com/MoYuanCN/",
      "color": "$TextColor",
      "size": 12,
      "padding": [5, 5, 5, 5]
