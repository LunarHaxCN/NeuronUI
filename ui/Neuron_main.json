{
  "type": "Menu",
  "title": {
    "name": "『Neuron』",
    "size": 15,
    "elevation": 4,
    "background": "$TitleBG",
    "padding": [
      4,
      4,
      4,
      4
    ],
    "colors": [
      "$TitleColor",
      "$TitleColor"
    ],
    "margins": [0, 0, 0, 0]
  },
