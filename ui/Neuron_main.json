    {
      "type": "TextView",
      "name": "Player",
      "color": "$TextColor",
      "size": 12,
      "padding": [
        5,
        5,
        5,
        5
      ],
      "open": "Neuron_player"
    },
    {
      "type": "TextView",
      "name": "Render",
      "color": "$TextColor",
      "size": 12,
      "padding": [
        5,
        5,
        5,
        5
      ],
      "open": "Neuron_render"
    },
    {
      "type": "TextView",
      "name": "Loophole",
      "color": "$TextColor",
      "size": 12,
      "padding": [
        5,
        5,
        5,
        5
      ],
      "open": "Neuron_loophole"
